int x, y, z;
bool b1, b2;
A a1, a2;

class A {
    int a, b, c;

    method void set1() { this.a = 1; }
}

main {
    x = 3;
    y = 4;
    z = 5;
    b1 = false;
    a1 = new A;
    a2 = new A;

    assert(x==3);
    assert(y == 4);
    assert(z == 5);
    assert(b1 == false);
    a1.set1();
    assert(a1.a == 1);
}
