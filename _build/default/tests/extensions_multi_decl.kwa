var int x, y, z;
var bool b1, b2;
var aaa a1, a2;

class aaa {
    attribute int a, b, c;

    method void set1() { this.a = 1; }
    method void printa() { print(this.a); }
}

main {
    x = 3;
    y = 4;
    z = 5;
    b1 = false;
    a1 = new aaa;
    a2 = new aaa;

    print(x);
    print(y);
    print(z);
    print(b1);
    a1.set1();
    a1.printa();
}
