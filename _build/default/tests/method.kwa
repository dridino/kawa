Point p;

class Point {
  int x;
  int y;

  method void constructor(int a, int b) {
    this.x = a;
    this.y = b;
  }

  method int somme() {
    return this.x + this.y;
    print(0);
  }

  method void addToY(int v) {
    this.y = this.y + v;
  }
}

main {
  p = new Point(2, 1);
  p.x = 3 + p.x;
  print(48 + p.somme());
  // affiche 54
  p.addToY(10);
  print(p.y);
  // affiche 11
}
